# .env.example
# Vibecode Studio Configuration

# Required: Gemini API Key for AI-powered execution
GEMINI_API_KEY=your-api-key-here

# Optional: Enable autonomous mode by default
VIBECODE_AUTO=0

# Optional: Confidence threshold for auto-approval (0.0-1.0)
VIBECODE_CONFIDENCE_THRESHOLD=0.8

# Optional: Custom audit log path
VIBECODE_AUDIT_LOG=.vibecode/autonomy_audit.log

# Optional: Maximum ReAct iterations
VIBECODE_MAX_STEPS=30

# Optional: Cache settings
VIBECODE_PROMPT_CACHE_SIZE=50
VIBECODE_RESULT_CACHE_TTL=30
